{
    "name": "marcrubio/flysystem-url-plugin",
    "description": "Flysystem plugin that generates URLs pointing to S3, Azure Blob, etc.",
    "type": "plugin",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Marc Rubio",
            "email": "marc.rubio@go1.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Mrubiosan\\FlyUrlPlugin\\": "src"
        }
    },
    "require": {
        "php": ">=5.6",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-azure": "^1.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "keywords": ["flysystem", "url", "aws", "s3" ,"azure", "blob"]
}
