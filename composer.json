{
    "name": "marcrubio/flysystem-url-plugin",
    "description": "Flysystem URL generation from Cloud providers like Aws S3 and Azure Blob",
    "type": "plugin",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Marc Rubio",
            "email": "marc.rubio@go1.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Mrubiosan\\FlyUrlPlugin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mrubiosan\\FlyUrlPlugin\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=5.6",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "league/flysystem-aws-s3-v3": "^1.0",
        "squizlabs/php_codesniffer": "^3.4",
        "league/flysystem-azure-blob-storage": "^0.1.5"
    },
    "suggest": {
        "league/flysystem-aws-s3-v3": "Support for AWS S3",
        "league/flysystem-azure-blob-storage": "Support for Azure Blob Storage"
    },
    "keywords": ["flysystem", "url", "aws", "s3" ,"azure", "blob"]
}
